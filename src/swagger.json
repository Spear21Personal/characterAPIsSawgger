{
    "swagger": "2.0",
    "info": {
        "version": "1.0.0",
        "title": "Typescript Swagger Express Boilerplpate",
        "description": "Boilerplate to setup Typescript API with Swagger",
        "license": {
            "name": "MIT",
            "url": "https://opensource.org/licenses/MIT"

        }   
    },
         "components": {
            "schemas": {
                "Classes": {
                    "type": "object",
                    "required": "name",
                  
                        
                    "properties": {
                        "id": {
                            "type": "string",
                            "description": "The auto-generated id of the classes"
                        },
                           
                        "name": {
                            "type": "string",
                            "description": "The classes Name"
                        }
                            
                    }
                        
                }
               
            }
    
     }
}